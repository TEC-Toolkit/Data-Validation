
# Find conflicting Conversion Factors
[?CF1, eov:conflictingCF, ?CF2] :-
    [?CF1, eov:sameCF, ?CF2] ,
    [?CF1, eo:hasAdditionalContext,  "Energy - Net CV"@en ] ,
    [?CF2, eo:hasAdditionalContext, "Energy - Gross CV"@en ] ,
    [?CF1, eo:targetUnit, ?TU] ,
    [?CF2, eo:targetUnit, ?TU] ,
    [?CF1, rdf:value, ?V1] ,
    [?CF2, rdf:value, ?V2],
    FILTER (?V1 < ?V2) .
